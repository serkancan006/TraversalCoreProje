@model TraversalCoreProje.CQRS.Commands.GuideCommands.CreateGuideCommand

@{
    ViewData["Title"] = "AddGuide";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>Yeni Rehber Ekleme sayfası</h1>

<form method="post">
    <label>Rehber Adı</label>
    <input type="text" class="form-control" asp-for="Name" />
    <br />

    <label>Rehber Açıklama</label>
    <input type="text" class="form-control" asp-for="Description" />
    <br />

    <button class="btn btn-outline-primary">Rehberi Kaydet</button>
</form>